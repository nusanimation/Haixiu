        <Slider Height="242" HorizontalAlignment="Left" Margin="909,44,0,0" Name="slider1" VerticalAlignment="Top" Width="46" Orientation="Vertical" SmallChange="1" TickPlacement="Both" ValueChanged="slider1_ValueChanged" MouseLeave="slider1_MouseLeave" AutoToolTipPlacement="BottomRight" />
        <Canvas Height="218" HorizontalAlignment="Left" Margin="12,717,0,0" Name="canvas2" VerticalAlignment="Top" Width="340" Background="#FF484848" />
