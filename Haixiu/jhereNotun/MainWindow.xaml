        <Button Content="Stop Record" Height="36" HorizontalAlignment="Left" Margin="886,641,0,0" Name="button2" VerticalAlignment="Top" Width="92" IsEnabled="False" Click="button2_Click" />
        <Image Height="50" HorizontalAlignment="Left" Margin="905,840,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="50" Source="/jhereNotun;component/red.png" ImageFailed="image1_ImageFailed" />
